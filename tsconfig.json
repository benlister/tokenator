{
  "compilerOptions": {
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2015"],
    "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
    "moduleResolution": "node",
    "outDir": "dist",
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.d.ts"]
}